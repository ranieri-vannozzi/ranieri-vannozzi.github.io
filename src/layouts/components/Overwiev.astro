---
const { className } = Astro.props
import bio from '@/config/bio.json'
---

<section
    class={className}
    id="overview"
>
    <div class="rounded border border-border p-12 lg:p-9 h-full">
        <ul class="flex items-center space-x-4">
            <h4 class="text-primary">Overview</h4>
        </ul>
        <p class="my-4">{bio.whoIAm}</p>
    </div>
</section>
