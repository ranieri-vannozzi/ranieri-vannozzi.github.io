---
const { className } = Astro.props;
import config from "@/config/config.json";

import Social from "./Social.astro";
---
<section class={className} id="contacts">
  <div class="rounded border border-border p-12 lg:p-9 h-full">
    <ul class="flex items-center space-x-4">
      <h4 class="text-primary">Contacts</h4>
    </ul>
    <!-- <p class="my-4">{config.curriculum.whoIAm}</p> -->
    <p class="my-4"><Social source={config.curriculum.contacts} className="social-icons mt-8" /></p>
  </div>
</section>
