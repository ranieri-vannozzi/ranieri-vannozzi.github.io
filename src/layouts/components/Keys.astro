---
const { className } = Astro.props
import keyCompetencies from '@/config/key-competencies.json'
---

<section
    class={className}
    id="keys"
>
    <div class="rounded border border-border p-12 lg:p-9 h-full">
        <ul class="flex items-center space-x-4">
            <h4 class="text-primary">Keys</h4>
        </ul>
        <p class="my-4">
            {keyCompetencies.map((key) => <li class="my-5">{key}</li>)}
        </p>
    </div>
</section>
